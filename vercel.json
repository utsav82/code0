{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/out/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "frontend/out/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "frontend/out/$1"
    }
  ]
}
